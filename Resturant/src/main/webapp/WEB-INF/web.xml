<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Resturant</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>foodDelv.chkpasswd</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>EditServlet</servlet-name>
  	<servlet-class>foodDelv.chngProf</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EditServlet</servlet-name>
  	<url-pattern>/chng</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>LogoutProfileServlet</servlet-name>
  	<servlet-class>foodDelv.profLogout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LogoutProfileServlet</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>SingupServlet</servlet-name>
  	<servlet-class>foodDelv.sgnupProf</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SingupServlet</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
</web-app>